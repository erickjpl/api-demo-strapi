{
  "kind": "collectionType",
  "collectionName": "inventories",
  "info": {
    "singularName": "inventory",
    "pluralName": "inventories",
    "displayName": "Inventory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "inventoryId": {
      "type": "uid",
      "required": true,
      "minLength": 36,
      "maxLength": 36
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product"
    },
    "warehouse": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::warehouse.warehouse"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive",
        "Sold Out"
      ],
      "required": true,
      "default": "Active"
    },
    "available": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true
    }
  }
}

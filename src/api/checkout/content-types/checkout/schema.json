{
  "kind": "collectionType",
  "collectionName": "checkouts",
  "info": {
    "singularName": "checkout",
    "pluralName": "checkouts",
    "displayName": "Checkout"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "checkoutId": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true,
      "minLength": 36,
      "maxLength": 36
    },
    "productId": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "baskets"
    },
    "quantity": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal",
      "required": true
    },
    "discount": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false,
      "type": "decimal"
    },
    "total": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "type": "decimal"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Active",
        "Returned",
        "Canceled"
      ],
      "required": true,
      "default": "Active"
    }
  }
}

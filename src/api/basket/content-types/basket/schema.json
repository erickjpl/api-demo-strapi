{
  "kind": "collectionType",
  "collectionName": "baskets",
  "info": {
    "singularName": "basket",
    "pluralName": "baskets",
    "displayName": "Basket",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "shippingAddress": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false,
      "maxLength": 120
    },
    "billingInformation": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false,
      "maxLength": 120
    },
    "promoCode": {
      "type": "string",
      "required": false,
      "maxLength": 12
    },
    "total": {
      "required": false,
      "type": "decimal",
      "default": 0
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Empty",
        "Active",
        "Inactive",
        "Abandoned",
        "Saved for Later",
        "On Hold",
        "Pending Review",
        "Ready for Checkout",
        "In Favorite",
        "Archived",
        "Fulfilled"
      ],
      "required": true,
      "default": "Active"
    },
    "paymentStatus": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Pending Payment",
        "Payment Processing",
        "Paid",
        "Approved",
        "Declined",
        "Cancelled",
        "Refund",
        "Refunded",
        "Waiting for Confirmation",
        "Payment Issue",
        "Payment Error",
        "Payment Overdue",
        "Authorization Pending",
        "Bank Confirmation Pending",
        "Partial Payment",
        "Scheduled Payment"
      ],
      "required": true,
      "default": "Pending Payment"
    },
    "notes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false,
      "maxLength": 120
    }
  }
}
